{
  "name": "prysma-ui",
  "version": "1.0.21",
  "description": "UI for Prysma LED control",
  "main": "index.js",
  "repository": "https://github.com/Rooknj/prysma-ui.git",
  "author": "Nick Rook <Nick_Rook@intuit.com>",
  "license": "MIT",
  "private": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "start": "ts-node --project scripts/tsconfig.json scripts/start",
    "build": "react-scripts build",
    "compress": "zip -r ui.zip build && tar -zcvf ui.tar.gz build",
    "test": "ts-node --project scripts/tsconfig.json scripts/test",
    "lint": "eslint 'src/**/*.{ts,tsx,json,md}' --config=.eslintrc.js",
    "reformat": "prettier './**/*.{js,ts,tsx,json,md}' --write",
    "eject": "react-scripts eject",
    "codegen": "graphql-codegen --config codegen.yml",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@commitlint/travis-cli": "8.2.0",
    "@graphql-codegen/add": "1.7.0",
    "@graphql-codegen/cli": "1.7.0",
    "@graphql-codegen/typescript": "1.7.0",
    "@graphql-codegen/typescript-operations": "1.7.0",
    "@graphql-codegen/typescript-react-apollo": "1.7.0",
    "@semantic-release/changelog": "3.0.4",
    "@semantic-release/git": "7.0.16",
    "@types/graphql": "14.2.3",
    "@types/jest": "24.0.18",
    "@types/lodash.throttle": "4.1.6",
    "@types/node": "12.7.5",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-router-dom": "4.3.5",
    "@types/styled-components": "4.1.19",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "6.3.0",
    "eslint-config-react-app": "5.0.2",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.17.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.14.3",
    "execa": "2.0.4",
    "husky": "3.0.5",
    "lint-staged": "9.3.0",
    "prettier": "1.18.2",
    "react-scripts": "3.0.1",
    "semantic-release": "15.13.24",
    "ts-node": "8.4.1",
    "typescript": "3.6.3",
    "yargs": "14.0.0"
  },
  "dependencies": {
    "@apollo/react-common": "3.1.1",
    "@apollo/react-hooks": "3.1.1",
    "@jaames/iro": "4.5.1",
    "@material-ui/core": "4.4.2",
    "@material-ui/icons": "4.4.1",
    "@material-ui/styles": "4.4.1",
    "apollo-cache": "1.3.2",
    "apollo-cache-inmemory": "1.6.3",
    "apollo-client": "2.6.4",
    "apollo-link": "1.2.13",
    "apollo-link-error": "1.1.12",
    "apollo-link-http": "1.5.16",
    "apollo-link-ws": "1.0.19",
    "apollo-utilities": "1.3.2",
    "graphql": "14.5.7",
    "graphql-tag": "2.10.1",
    "lodash.throttle": "4.1.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-router-dom": "5.0.1",
    "styled-components": "4.3.2",
    "subscriptions-transport-ws": "0.9.16",
    "typeface-roboto": "0.0.75"
  }
}
